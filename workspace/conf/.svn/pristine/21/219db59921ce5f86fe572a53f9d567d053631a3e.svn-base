.top{
    position: fixed;
    height: 47px;
    line-height: 47px;
    /*padding: 0 25px;*/
    background: #fff;
    border-bottom: 1px solid #eee;
    margin-top: 10px;
    z-index: 1000;
    width: 83%;
}
.top>span{
    height:20px;
    line-height: 20px;
    display: inline-block;
    border-right:1px solid #e9e9e9;
    text-align:  center;
    padding: 0 30px;
    cursor:pointer;
}
.top>span.active{
    color:#da0e00;
}
.top>span img{
    margin-left:10px;
}
.meet_search{
    float: right;
}
.meet_search .h_search,
.meet_search .h_type{
    margin: 5px 10px;
}
/*会议列表*/
.mList{
    margin-bottom: 20px;
}
.mList li{
    position: relative;
    /*height: 156px;*/
    padding: 17px 12px;
    background: #fff;
    margin-top: 20px;
    overflow: hidden;
}
.mList .l_img,.mList .l_info{
    float:left;
}
.mList .l_img img{
    width:260px;
    height: 156px;
    margin-right: 10px;
}
.l_info .title{
    display: block;
    max-width: 500px;
    font-size: 16px;
    color:#333;
    line-height: 32px;
    margin-bottom: 10px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.l_info .title:hover{
    color: #da0e00;
}
.l_info span{
    font-size: 12px;
    color:#999cab;
}
.l_info .time{
    color:#999cab;
}
.l_info p img{
    margin-right: 9px;
}
.l_info .type .tName{
    height: 20px;
    line-height: 20px;
    text-align: center;
    background: #da0e00;
    color:#fff;
    border-radius: 3px;
    display: inline-block;
    padding:0 8px;
    margin-right: 10px;
}
.l_info .type .tPost{
    height: 20px;
    line-height: 20px;
    padding: 0 8px;
    text-align: center;
    background: #da0e00;
    color:#fff;
    border-radius: 3px;
    display: inline-block;
}
.l_name{
    position: absolute;
    right: 10px;
    line-height: 140px;

}
.l_name span{
    width: 60px;
    display:  inline-block;
    text-align: center;
    line-height: 25px;
    margin-right: 30px;
}
.l_name span i{
    color:#bd0f10;
    font-size: 16px;
}
.l_btn{
    position: absolute;
    bottom:17px;
    right:0;
    width: 270px;
}
.l_btn span{
    margin-right:35px;
    cursor:pointer;
}
.l_btn .share{
    color:#bd0f10;
    padding-left:20px;
    background: url(../img/share.png) left center no-repeat;
}
.l_btn .del{
    padding-left:20px;
    background: url(../img/del.png) left center no-repeat;
}
.l_btn .manage{
    padding-left:20px;
    margin-right:26px;
    background: url(../img/manage.png) left center no-repeat;
}
/*page*/
.pagination{
    /*margin: 40px auto 0;*/
    /*display: block;*/
    /*width: 345px;*/
    height: 34px;
    float: right;
    margin-right: 100px;
}
.pagination>.active>a, .pagination>.active>span, .pagination>.active>a:hover, .pagination>.active>span:hover, .pagination>.active>a:focus, .pagination>.active>span:focus{
    background-color:#da0e00;
    border-color:#da0e00;
}
.pagination>li>a:hover, .pagination>li>span:hover, .pagination>li>a:focus, .pagination>li>span:focus{
    color: #da0e00;
}
.pagination>li>a, .pagination>li>span{
    color: #da0e00;
}

.maskInfo2 img{
    display: block;
    width: 150px;
    height: 150px;
    margin: 10px auto 20px;
}
.maskInfo2 p{
    text-align: center;
    line-height: 30px;
    margin-top: 15px;
}
.maskInfo2 .mask2_url{
    display: block;
    width: 80%;
    line-height: 20px;
    margin: 0 auto;
    text-align: center;
    color: #da0e00;
    border: 0;
    box-sizing: border-box;
}
.maskInfo2 .btns2{
    padding: 20px 0;
    width: 200px;
    margin: 0 auto;
    overflow: hidden;
    cursor: pointer;
}
.maskInfo2 .btns2 span{
    float: left;
    width: 80px;
    height: 35px;
    line-height: 35px;
    border-radius: 5px;
    text-align: center;
    box-sizing: border-box;
}
.maskInfo2 .btns2 span.mask2_close {
    border:1px solid #da0e00;
    margin-right: 40px;
}
.maskInfo2 .btns2 span.mask2_copy{
    background: #da0e00;
    color: #fff;
}















