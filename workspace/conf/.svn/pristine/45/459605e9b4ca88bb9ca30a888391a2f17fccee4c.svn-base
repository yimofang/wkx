/*Survey页面*/
/*create*/
.create{
	width:100%;
	height:0.88rem;
	line-height: 0.88rem;
	text-align: center;
	background:#fff;
	margin-top: 0.88rem;
	color:#bd0f10;
	box-shadow: 0 0.01rem 0.05rem #ccc;
}
.addSurvey{
	width: 100%;
	height:0.8rem;
	line-height: 0.8rem;
	text-align: center;
	font-size: 0.28rem;
	color:#bd0f10;
	background: #fff;
	border-bottom:0.01rem solid #eee;
}
/*content*/
.content{
	width:100%;
	text-align: center;
}
.content .unhave{
	width: 100%;
    text-align: center;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}
.content .imgTit,.content .imgTit2{
	line-height: 1rem;
	color:#999cab;
}
.creNewQues{
	position: fixed;
	width:100%;
	height:0.88rem;
	line-height: 0.88rem;
	text-align:center;
	font-size: 0.28rem;
	bottom:0;
	background:#1b1b1b;
	color:#fff;
} 
/*content_list*/
.content_list{
	width:100%;
	height:auto;
	background:#fff;
	margin-top:0.24rem;
	font-size: 0.28rem;
}
.content_list div{
	width:90%;
	height:0.88rem;
	line-height: 0.88rem;
	border-bottom:0.01rem solid #efefef;
	margin:0 auto;
}
.content_list div p{
	float: left;
}
.content_list div .list_date{
	margin:0 1.04rem;
}
.content_list div button{
	width:0.24rem;
	height:0.24rem;
	border:none;
	float:right;
	background: #ccc;
	margin-top:0.32rem;
}
.surMessage{
	width: 90%;
	margin:0 auto 0.24rem;
	padding:3% 5%;
	background:#fff;
}
.surMessage .surTit{
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	font-size: 0.36rem;
	width: 90%;
	display: inline-block;
	text-align: center;
}
.surMessage .surEdit{
	width: 0.3rem;
    height: 0.3rem;
    background: url(../img/edit.png) no-repeat;
    background-size: cover;
    background-position: center;
    margin-top: 0.1rem;
    float: right;
}
.surMessage .surMsg{
	overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp:3;
    -webkit-box-orient: vertical;
    line-height: 0.5rem;
    font-size: 0.26rem;
}
.haveContent{
	/*margin-top: 0.88rem;*/
    width: 100%;

}
.haveContent .conList{
	width:100%;
	height:7rem;
	margin:0 auto 2rem;
	overflow: auto;
}
.haveContent .conList .singleCon,
.haveContent .conList .doubleCon,
.haveContent .conList .blankCon{
	width: 90%;
    border-bottom: 0.01rem solid #efefef;
    padding: 0.2rem 5%;
    background: #fff;
}
.singleCon .sinTop,.doubleCon .douTop,.blankCon .blkTop{
	font-size:0.28rem;
	font-weight: bold;
	line-height: 0.73rem;
	height:0.73rem;
}
.singleCon .sinTop .sinTitle,
.doubleCon .douTop .douTitle,
.blankCon .blkTop .blkTitle{
	float: left;
    width: 85%;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
}
.singleCon .sinOption{
	font-size:0.26rem;
	line-height: 0.54rem;
}
.singleCon .sinOption input,.doubleCon .douOption input{
	margin-right:0.18rem;
}
.edit{
    width: 0.3rem;
    height: 0.3rem;
    background: url(../img/edit.png) no-repeat;
    background-size: cover;
    background-position: center;
    margin-top: 0.19rem;
    float:left;
}
.delete{
    float: right;
    width: 0.3rem;
    height: 0.3rem;
    background: url(../img/delete.png) no-repeat;
    background-size: cover;
    margin-top: 0.19rem;
}
.blkAnswer{
	width:100%;
	height:0.8rem;
	font-size:0.26rem;
	background: #f4f4f4;
	padding-left: 0.1rem;
}
i{color:#bd0f10;}
/*surveyCreate页面*/

/*survey*/
.survey{
	width:100%;
	height:auto;
	margin-top:0.88rem;
	font-size: 0.28rem;
}
.survey b{color:#bd0f10;margin-right: 0.5rem;}
.sTitle,.sExplain{
	width: 90%;
	height:0.65rem;
	line-height: 0.65rem;
	font-size:0.24rem;
	margin:0 5%;
}
.sTitle>span{
	color:#bd0f10;
	z-index: 1000;
}
.wTitle{
	width: 90%;
	height:0.88rem;
	line-height: 0.88rem;
	background:#fff;
	font-size: 0.26rem;
	padding:0 5%;
}
.wTitle input{
 	height: 0.3rem;
    width: 100%;
    z-index: 1;
    font-size:0.26rem;
 }
.wExplain{
	width:90%;
	height:2.1rem;
	padding: 0 5%;
	background: #fff;
}
.wExplain textarea{
	width: 100%;
    height: 1.6rem;
    padding: 0.25rem 0;
    font-size: 0.26rem;
    border: none;
}
.surNum{
	width: 90%;
    height: 0.9rem;
    line-height: 0.9rem;
    text-align: right;
    background: #fff;
    padding: 0 5%;
}



/*submit*/
.btn {
	width:90%;
	height:0.98rem;
	line-height: 0.98rem;
	margin:0.5rem 5% 0;
	background:#1b1b1b;
	color:#fff;
	border-radius:0.1rem;
	font-size:0.3rem;
	text-align: center;
}
.btn a{color:#fff;font-size:0.28rem;}


/*surveyEdit页面*/
/*edit*/
.editTitle{
	width:90%;
	height:auto;
	background:#fff;
	margin-top: 0.88rem;
	padding:0.46rem 5%;
	border-bottom:0.01rem solid #efefef;
}
.editTitle p:first-child{
	font-size:0.34rem;
	margin-bottom:0.16rem;
}
.editTitle p:last-child{
	font-size:0.24rem;
	color:#c0c2cd;
}
.editProblem{
	width:100%;
	height: auto;
	background:#fff;
	font-size:0.28rem;
}
.editProblem ul li{
	position: relative;
	width:90%;
	height:0.88rem;
	line-height: 0.88rem;
	margin:0 auto;
	border-bottom:0.01rem solid #efefef;
}
.editProblem ul li span:nth-child(2){
	position: absolute;
	right:0.8rem;
}
.editProblem ul li button{
	position: absolute;
	right:0rem;
	width:0.24rem;
	height:0.24rem;
	line-height: 0.24rem;
	border:none;
	top:0.32rem;
	background: url(../img/false.png);
	background-repeat: no-repeat;
	background-size: cover;
}
.bottomBtn{
	position: fixed;
	bottom: 0;
	width:100%;
	/*height: 0.88rem;*/
	font-size: 0.28rem;
}
.bottomBtn .only{
	text-align: center;
    margin-bottom: 0.6rem;
    font-size: 0.26rem;
}
.bottomBtn .only .onlySpan{
	position: relative;
	width: 0.3rem;
	height: 0.3rem;
	line-height: 0.3rem;
	margin-right: 0.18rem;
}
.bottomBtn .only .onlySpan .onlyCheck{
	position: absolute;
	width: 0.26rem;
	height: 0.26rem;
	visibility: hidden;
}
.bottomBtn .only .onlySpan .onlyCheck+label{
	display: inline-block;
	width: 0.26rem;
	height: 0.26rem;
	border: 0.02rem solid #b9b9b9;
}
.onlyCheck:checked+label{ 
	background: url(../img/only.png) no-repeat center center;
	background-size: 0.3rem 0.3rem;
	border: 0.02rem solid #1b1b1b;
}
.pubQues{
    width: 50%;
    height: 0.88rem;
    line-height: 0.88rem;
    color: #fff;
    background: #1b1b1b; 
    float:left;
    text-align: center;
}
.creQues{
    width: 50%;
    height: 0.88rem;
    line-height: 0.88rem;
    color: #fff;
    background: #bd0f10;
    float:left;
    text-align: center;
}
/*隐藏弹框：添加新题*/
.addQuestion{
	width:100%;
	position: absolute;
	top:0;
	bottom:0;
	left:0;
	right:0;
	z-index:999;
	background:rgba(0,0,0,.2);
}
.addQuestion div{
	position: fixed;
	width:100%;
	bottom:0;
	z-index:1000;
}
.addQuestion div li{
	width:100%;
	height:0.88rem;
	line-height: 0.88rem;
	text-align: center;
	border-bottom:0.01rem solid #efeffe;
	background: #fff;
	font-size:0.28rem;
}

#cancel{
	border:none;
}
/*删除弹框*/
.deleBg,.surveyBg,.subbg{
	position: fixed;
	width: 100%;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 999;
    background: rgba(0,0,0,.2);
}
.deleBgCon,.surveyBg .surCon{
	position: absolute;
    z-index: 1000;
    width: 5.57rem;
	padding-bottom: 0.5rem;
    text-align: center;
    top:50%;
    left:50%;
    transform: translate(-50%,-50%);
    border-radius: 0.1rem;
    background: #fff;
}
.top,.surCon .top{
	width: 5.57rem;
    height: 0.9rem;
    line-height: 0.9rem;
    text-align: center;
    background: #bd0f10;
    font-size: 0.32rem;
    color: #fff;
    border-top-left-radius: 0.1rem;
    border-top-right-radius: 0.1rem;
}
.surInfo{
	margin:0.1rem auto;
}
.surInfo input{
	width: 90%;
    height: 0.6rem;
    line-height: 0.6rem;
    font-size: 0.24rem;
    background: #e5e5e5;
    border-radius: 0.05rem;
    padding:0 0.1rem;
}
.surInfo textarea{
	    width: 90%;
    height: 1rem;
    background: #e5e5e5;
    border: none;
    border-radius: 0.05rem;
    font-size: 0.24rem;
    margin-top: 0.2rem;
    padding:0.1rem;
    line-height: 0.4rem;
}
.deleBgCon .sureDelCon .sureConP{
	font-size: 0.3rem;
    margin: 0.5rem auto;
    text-align: center;
}
.deleBgCon .sureDelCon .sureConBtn{
	margin:0 auto;
	width:4.7rem;
}
.sBtn{

    width: 90%;
    margin: 0 auto;
}
.deleBgCon .sureDelCon .sureConBtn span,.surveyBg .sBtn span{
	width: 2.3rem;
    height: 0.68rem;
    line-height: 0.68rem;
    text-align: center;
    display: inline-block;
    color: #fff;
    border-radius: 0.1rem;
}
.deleBgCon .sureDelCon .sureConBtn .conCel,.s_cancel{
	float:left;
	background:#1b1b1b;
}
.deleBgCon .sureDelCon .sureConBtn .conSure,.s_sure{
	float:right;
	background: #bd0f10;
}
/*surveySingle页面*/
/*singleTitle*/
.singleTitle{
	width:100%;
	height:0.88rem;
	background:#fff;
	font-size:0.28rem;
	margin-bottom: 0.24rem;
}
.singleTitle div{
	width:90%;
	height:0.88rem;
	line-height: 0.88rem;
	margin:0 auto;
}
.singleTitle div span,
.singleOption div>span
{
	width: 1rem;
    display: inline-block;
    text-align: left;
}
.singleTitle div input
{
    width: 4rem;
    height: 0.88rem;
    padding-left: 0.1rem;
    float:right;
    text-align: right;
    margin-bottom:0.24rem;
    margin-right: 0.3rem;
}
.singleOption{
	width:100%;
	height: auto;
	background:#fff;
	font-size: 0.28rem;
	/*padding:5% 0;*/
}
.singleOption div,.editSinOptList div{
	width:90%;
	height:0.88rem;
	line-height: 0.88rem;
	margin:0 auto;
	position:relative;
	border-bottom:0.01rem solid #efefef;
}
.singleOption input{
	position:absolute;
    width: 4rem;
    height: 0.5rem;
    padding-left: 0.1rem;
    right:0.8rem;
    text-align: right;
    top:0.2rem;
}
.singleOption button,.doubleOption button{
	width:0.24rem;
	height: 0.24rem;
	background: url(../img/false.png) no-repeat;
	background-size: cover;
	float:right;
	margin-top: 0.32rem;
	border:none;
}
.addOption {
	width:100%;
	height:0.88rem;
	line-height: 0.88rem;
	/*padding:0 10%;*/
	text-align: center;
	font-size:0.28rem;
	background: #fff;
	color:#bd0f10;
}
.addOption p{
	/*width:1.5rem;
	float:right;*/
}
input::-webkit-input-placeholder{
	font-size:0.26rem;
	padding-left: 0.1rem;
}
input::-moz-input-placeholder{
	font-size:0.26rem;
}

/*surveyDouble页面*/
.doubleTitle{
	width:100%;
	height:0.88rem;
	background:#fff;
	font-size:0.28rem;
	margin-bottom:0.24rem;
}
.doubleTitle div{
	width:90%;
	height:0.88rem;
	line-height: 0.88rem;
	margin:0 auto;
}
.doubleTitle div span,.blankTitle span{
	width: 1rem;
    display: inline-block;
    text-align: left;
}
.doubleOption div>span{
	width: 1rem;
    display: inline-block;
    text-align: left;
}
.doubleTitle div input,.blankTitle input{
	    /*border: 0.01rem solid #b1afaf;*/
    width: 4rem;
    height: 0.88rem;
    padding-left: 0.1rem;
    text-align: right;
    margin-right: 0.3rem;
    float: right;
}
.doubleOption{
	width:100%;
	height: auto;
	background:#fff;
	font-size: 0.28rem;
	margin-bottom: 0.24rem
}
.doubleOption div{
	position: relative;
	width:90%;
	height:0.88rem;
	line-height: 0.88rem;
	margin:0 auto;
	/*border-bottom:0.01rem solid #efefef;*/

}
.douOptList{
	border-bottom:0.01rem solid #efefef;
}
.doubleOption input{
	position: absolute;
	/*border: 0.01rem solid #b1afaf;*/
    width: 4rem;
    height: 0.5rem;
    padding-left: 0.1rem;
    right: 0.8rem;
    text-align: right;
    top:0.2rem;

}

.doubleChoice{
	width:100%;
	height:2rem;
	background: #fff;
	font-size:0.28rem;
}
.doubleChoice div{
	width:90%;
	height:0.88rem;
	margin:0 auto;
	line-height: 0.88rem;
}
.doubleChoice div select{
    width: 4rem;
    height: 0.5rem;
    margin-top: 0.19rem;
    float:right;
    font-size:0.26rem;
    height: 0.5rem;
    padding-left: 0.1rem;
    appearance:none;
    -moz-appearance:none; 
    -webkit-appearance:none;
    /*border:0.01rem solid #efefef;*/
    padding-right:0.2rem;
    background:url(../img/select.png) no-repeat;
    background-position: right;
    background-size: 0.2rem 0.2rem;
}
.doubleChoice div select option{
}
/*surveyBlank*/
.blankTitle{
	width:100%;
	height:0.88rem;
	background:#fff;
	font-size:0.28rem;
	/*border-bottom:0.01rem solid #efefef;*/
	margin-bottom: 0.24rem;
}
.blankTitle div{
	width:90%;
	height:0.88rem;
	line-height: 0.88rem;
	margin:0 auto;
}
.blankChoice{
	width:100%;
	/*height:1.3rem;*/
	background: #fff;
	font-size:0.28rem;
}
.blankChoice div{
	width:90%;
	height:1.76rem;
	line-height:0.88rem;
	margin:0 auto;
	
}
.blankChoice div>span{
	width: 33.3%;
    float: left;
}
.blankChoice div>label{
	position:relative;
	width: 100%;
    display: inline-block;
    float: left;
    
}
.blankChoice div>label.lab1{
	border-bottom:0.01rem solid #efefef;
}
.blankChoice div>label span{
	margin-left: 0.1rem;
}


/*surveyPreview页面*/
/*previewCue*/
.previewCue{
	width:90%;
	color:#868997;
	font-size:0.28rem;
	text-align: center;
	padding:5%;
	background: #fff;
	margin-top:0.88rem;
	line-height: 0.58rem;
}
.previewContent{
	margin-top: 0.25rem;
	width:90%;
	height:auto;
	padding:0 5% 5%;
	background:#fff;
}
.previewContent .comName{
	width:92%;
	height:0.8rem;
	line-height: 0.8rem;
	padding:0 4%;
	background:#f4f4f4;
}
.previewContent ul li{
	width:90%;
	height:auto;
	padding:5% 0;
	border-bottom:0.01rem solid #efefef;
}
.previewContent ul li p{
	font-size:0.26rem;
	line-height: 0.64rem;
}
.previewContent ul li p:first-child{
	font-size:0.28rem;
}
.previewContent input,.answer input{
	width:0.32rem;
	height:0.32rem;
	line-height: 0.32rem;
	margin-right: 0.13rem;
}
/*answer*/
.answer{
	margin:0.3rem;
	text-align: center;
	font-size:0.28rem;
}

/*submit*/
.pubBtn{
	width: 100%;
	height:0.85rem;
	line-height: 0.85rem;
	text-align: center;
	font-size:0.3rem;
	color:#fff;
	background: #000;
	margin-top:0.83rem;
}

/*隐藏:报名信息*/
.bg{
	position: absolute;
	width:100%;
	height:100%;
	top:0;
	bottom:0;
	left:0;
	right: 0;
	z-index: 999;
	background: rgba(0,0,0,.2);
}
.bg .cue{
	position: fixed;
	z-index: 1000;
	width:5.57rem;
	height:4.3rem;
	margin-left:0.97rem;
	margin-top:5rem;
	border-radius: 0.1rem;
	background: #fff;
}
.bg .cue .cueTitle{
	width:5.57rem;
	height:0.9rem;
	line-height: 0.9rem;
	text-align: center;
	background: #bd0f10;
	font-size:0.32rem;
	color:#fff;
	border-top-left-radius: 0.1rem;
	border-top-right-radius: 0.1rem;
}
.bg .cue .cueContent{
	padding:8% 6%;
	font-weight: 600;
	text-align: center;
	font-size:0.3rem;
}
.bg .cue .cueContent input{
	width:3.22rem;
	height: 0.68rem;
	line-height: 0.68rem;
	background: #000;
	color:#fff;
	margin-top: 0.4rem 0 0.16rem;
	border-radius: 0.1rem;
}