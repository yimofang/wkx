*{padding:0;margin:0;}
.myHead{position: relative;width:100%;height:4.8rem;text-align: center;background:url(../img/myHeadBg.png) no-repeat center center;background-size:8rem 4.8rem;}
.myHead .myPhoto img{position: relative;width:1.9rem;height:1.9rem;border-radius: 1.9rem;border:0.01rem solid #efefef;top: 0.93rem;margin: 0 auto;display: inline-block;}
.myHead .myName{position: relative;font-size: 0.32rem;color:#fff;top: 0.3rem;display: block;margin:0 auto;text-align: center;}
.myHead select{position: relative;top: 1.5rem;color:#333;background:#fff;height:0.4rem; outline:none;}
select option{border-bottom: 1px solid #eee; outline:none;}

.myInfo{width:100%;height:auto;margin-top:0.52rem;}
.myInfo ul li{width:100%;height:0.88rem;line-height: 0.88rem;padding:0 5%;font-size:0.28rem;background:#fff;border-bottom:0.01rem solid #efefef;}
.myInfo ul li a{display: block;width: 100%;height: 100%;}
.myInfo .meetRecord{padding-left: 0.5rem;background: url(../img/myIcon0.png) no-repeat;background-position: left;background-size: 0.3rem 0.34rem;}
.myInfo .infoCenter{padding-left: 0.5rem;background:url(../img/myIcon1.png) no-repeat;background-position:left;background-size: 0.3rem 0.34rem;}
.myInfo .modifyPW{padding-left: 0.5rem;background:url(../img/myIcon2.png) no-repeat;background-position:left;background-size: 0.3rem 0.34rem;}
.myInfo .exit{padding-left: 0.5rem;background:url(../img/myIcon3.png) no-repeat;background-position:left;background-size: 0.3rem 0.34rem;}
.myInfo .infoMore,.myInfo .moMore,.myInfo .reMore{display: inline-block;float:right;width:0.1rem;height:0.18rem;background:url(../img/more.png) no-repeat;background-size: cover;margin-top: 0.35rem;}

.UploadImg{padding-top:1.2rem;}
/*会议记录*/
.Content{top:0rem;}
.bottom_tip {
    text-align: center;
    font-size: 0.26rem;
    line-height: 0.5rem;
    margin: 0.2rem 0;
}

/*修改密码*/
.modifyCon{margin-top: 0.24rem;}
.modifyCon ul li{width: 90%; height: 0.88rem;line-height: 0.88rem;background: #fff;padding: 0 5%;}
.modifyCon .modNew{margin-top: 0.16rem;border-bottom:0.01rem solid #efefef;}
.modifyCon .PW{font-size:0.3rem;width: 1.8rem;display: inline-block;}
.modifyCon .PWPut{width: 4.5rem;font-size:0.26rem;}
.del{width: 0.24rem;height: 0.24rem;float: right;margin-top: 0.33rem;background:url(../img/false.png) no-repeat;background-size: cover;}
.btn{width: 90%;height: 0.98rem;line-height: 0.98rem;background: #1b1b1b;color: #fff;text-align: center;font-size: 0.32rem;border-radius: 0.1rem;margin: 1.3rem auto 0;}

/*消息中心*/
.msgList .infoList{position: relative;height: 1.2rem;width: 90%;padding: 0.3rem 5%;border-bottom: 0.01rem solid #eaeaea;}
.infoList .l_msh{font-size: 0.26rem;color:#bd0f10;padding-left:0.38rem;float:left;background:url(../img/msg.png) left center no-repeat;background-size: 0.26rem 0.26rem}
.infoList .l_date{font-size:0.22rem;color:#b3b3b3;float:right;}
.infoList p{font-size: 0.24rem;color:#525252;line-height: 0.38rem;margin-top: 0.5rem;text-overflow: ellipsis;overflow: hidden;display: -webkit-box;  -webkit-line-clamp: 2;  -webkit-box-orient: vertical; }

.noMeet p,.unMsg p{font-size: 0.26rem;color: #999cab;margin-top: 0.4rem;}
.noMeet,.unMsg{    position: absolute;top:50%;left:50%;text-align:center;width:100%;transform:translate(-50%,-50%);}

/*消息详情*/


/*图片剪裁*/
		
			
section{font-size:0.9rem;    margin-bottom: 0.3rem;}
#headimg{width:1.9rem;
    height:1.9rem;margin:auto;background-size:100% 100%!important;background-repeat:no-repeat;background-position:center center;overflow:hidden;}
.btn2{position:relative;z-index:10;height:1.6rem;line-height:1.6rem;margin:auto;font-size:0.8rem;color:#fd1142!important;}
.btn2 img{width:100%;height:100%;}

/*截图上传页面*/
.clipbg{position:fixed;background:black;top:0;z-index:999;width:100%;height:100%;left:0;}
.loading{position:absolute;top:40%;width:38%;left:31%;height:1.6rem;line-height:1.6rem;z-index:99999;text-align:center;color:#ffffff;border-radius:0.2rem;background:#9f9f9f;font-size:0.3rem;}
.clipbg #clipArea{width:100%;height:80%;margin:auto;}
.clipbg .footer{width:90%;position:fixed;left:5%;bottom:0px;text-align:center;font-size:0.3rem;}
.clipbg dl{background:#ffffff;border-radius:0.1rem;overflow:hidden;margin-bottom:0.6rem;}
.clipbg dd{position:relative;height:0.8rem;line-height:0.8rem;border-bottom:1px solid #999999;}
.clipbg .back{height:0.8rem;line-height:0.8rem;border-radius:0.1rem;margin-bottom:0.4rem;background:#ffffff;}
.clipbg dd input{position:absolute;width:100%;height:100%;top:0;left:0;z-index:11;filter:alpha(opacity=0);-moz-opacity:0;-khtml-opacity:0;opacity:0;}

/*选择名称*/
#showBank{font-size: 0.32rem;color:#fff}
.ios-select-widget-box #iosSelectTitle{display: block}