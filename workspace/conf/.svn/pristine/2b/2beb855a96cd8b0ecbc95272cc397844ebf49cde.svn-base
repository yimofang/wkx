.Header{position:fixed;top:0;left:0;width: 100%;height: 0.88rem;line-height:0.88rem;font-size:0.36rem;background: #da0e00;color: #fff;text-align: center;z-index: 1000;}
.Header a:nth-child(1){position: absolute;top:0;left:0;width: 0.88rem;height: 0.88rem;background: url(../img/index.png) no-repeat 0.25rem 0.25rem;background-size: 10rem;}
.add{position:fixed;}
/*footer*/
.Footer{position: fixed;bottom: 0;left: 0;width:100%;height: 0.98rem;background: #1b1b1b;}
.Footer ul{overflow: hidden;}
.Footer ul li{float: left;width: 33.33%;}
.Footer ul li i{display: block;width: 0.5rem;height: 0.5rem;margin: 0 auto;}
.Footer ul li i.Icon1{background: url(../img/index.png) no-repeat 0.06rem -1.9rem;background-size: 10rem;}
.Footer ul li i.Icon2{background: url(../img/index.png) no-repeat 0.06rem -2.9rem;background-size: 10rem;}
.Footer ul li i.Icon3{background: url(../img/index.png) no-repeat 0.06rem -3.9rem;background-size: 10rem;}
.Footer ul li p{height:0.48rem;line-height:0.48rem;font-size:0.22rem;color:#777984;text-align: center;}
.Footer ul li p.active{color: #bd0f10;}

.user_info{margin-top: 1rem;}
.user_info ul {padding: 0 0.24rem;background: #fff; }
.user_info ul li{height: 0.89rem;line-height:0.89rem;}
.user_info ul li:first-child{border-bottom: 0.01rem solid #f3f4f6;}
.user_info ul li span{float: left;width: 20%;line-height: 0.89rem;font-size: 0.3rem;text-align: center;}
.user_info ul li input{float: left;width: 65%;height: 0.88rem;font-size: 0.3rem;padding: 0 5%;box-sizing: border-box;}
.user_info ul li input::-webkit-input-placeholder {font-size: 0.3rem;color: #b1b1b1;}
.user_info ul li i{float: right;width: 15%;height: 0.89rem;background: url(../img/close.png) no-repeat 0.45rem;background-size: 0.36rem;}
.user_info .login{height:1rem;line-height:1rem;margin: 1.3rem 0.24rem 0 ;border-radius: 0.08rem;background: #1b1b1b;color: #fff;text-align: center;font-size: 0.32rem;box-shadow: 0.01rem 0.01rem 0.01rem 0.01rem #cccbcb;}











