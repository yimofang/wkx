/*******************************名单管理*********************************************/
/*header*/
.Header .SearchIocn{float: right;width: 0.4rem;height: 0.4rem;margin-right:0.24rem;margin-top: 0.24rem;background: url(../img/search.png) no-repeat;background-size: cover;}
/*NameList*/
.NameList{margin-top: 0.88rem;position: fixed;top: 0;bottom: 0;width: 100%;overflow: scroll;}
.NameList .TitTab{margin-bottom: 0.16rem;background: #fff;}
.NameList .TitTab li{float: left;width: 33.33%;}
.NameList .TitTab li span{display: block;margin:0 0.35rem;font-size: 0.3rem;padding: 0.26rem 0 0.22rem;text-align: center;color: #393939;}
.NameList .TitTab li span b{font-size: 0.28rem;font-weight: normal;}
.NameList .TitTab li span.active{color: #bd0f10;border-bottom: 0.04rem solid #bd0f10;box-sizing: border-box;}
.NameList .NameCon{background: #fff;}
.NameList .NameCon .NameCon1 .AllListTit,
.NameList .NameCon .NameCon2 .QianListTit,
.NameList .NameCon .NameCon3 .ChuanListTit{padding: 0 0.32rem;border-bottom: 0.01rem solid #efefef;box-sizing: border-box;}
.NameList .NameCon .NameCon1 .AllListTit li,
.NameList .NameCon .NameCon2 .QianListTit li,
.NameList .NameCon .NameCon3 .ChuanListTit li{float: left;font-size: 0.28rem;line-height: 0.71rem;color: #bd0f10;text-align: center;}
.NameList .NameCon .NameCon1 .AllListTit li:first-child{width: 20%}
.NameList .NameCon .NameCon1 .AllListTit li:nth-child(2){width: 33%}
.NameList .NameCon .NameCon1 .AllListTit li:nth-child(3){width: 30%}
.NameList .NameCon .NameCon1 .AllListTit li:last-child{width: 17%}
/*.NameList .NameCon .NameCon1 .AllListCon,*/
/*.NameList .NameCon .NameCon2 .QianAllList,*/
/*.NameList .NameCon .NameCon3 .ChuanAllList{height: 7rem;overflow-y:auto;}*/
.NameList .NameCon .NameCon1 .AllListCon li,
.NameList .NameCon .NameCon2 .QianAllList li,
.NameList .NameCon .NameCon3 .ChuanAllList li{margin: 0 0.32rem;border-bottom: 0.01rem solid #efefef;box-sizing: border-box;text-align: center; overflow: hidden;}
.NameList .NameCon .NameCon1 .AllListCon li span,
.NameList .NameCon .NameCon2 .QianAllList li span,
.NameList .NameCon .NameCon3 .ChuanAllList li span{float: left;width: 30%;line-height:0.88rem;font-size: 0.26rem;color: #333;}
.NameList .NameCon .NameCon1 .AllListCon li:last-child,
.NameList .NameCon .NameCon2 .QianAllList li:last-child,
.NameList .NameCon .NameCon3 .ChuanAllList li:last-child{margin-bottom: 1rem;}
.NameList .NameCon .NameCon1 .AllListCon li span:first-child{width: 20%}
.NameList .NameCon .NameCon1 .AllListCon li span:nth-child(2){width: 33%}
.NameList .NameCon .NameCon1 .AllListCon li span:nth-child(3){width: 30%}
.NameList .NameCon .NameCon1 .AllListCon li span:last-child{width: 17%}
.NameList .NameCon .NameCon1 .AllListCon li span img,
.NameList .NameCon .NameCon3 .ChuanAllList li span img{display: block;width: 0.27rem;height: 0.27rem;margin: 0.25rem auto;}
.NameList .NameCon .NameCon2 .QianListTit li,
.NameList .NameCon .NameCon3 .ChuanListTit li{width: 33.33%;}
.NameList .NameCon .NameCon2 .QianAllList li span,
.NameList .NameCon .NameCon3 .ChuanAllList li span{width: 33.33%;}
.NameList .NameCon .NameCon3 .PCcaozuo{margin-top: 30%;background: #fafafa;}
.NameList .NameCon .NameCon3 .PCcaozuo img{display: block;width:100%;margin: 0 auto;}
.NameList .NameListAdd{display: block;width:100%;height:0.9rem;line-height:0.9rem;text-align:center;position: fixed;bottom: 0;left: 0;background: #1b1b1b;color: #fff;}
/*隐藏添加名单*/
.addName_content{width: 100%;height:auto;background:#fff;margin-top: 0.88rem;}
.addName_content b{color:#bd0f10;}
.addName_content div{width:90%;height:auto;margin:0 auto;}
.addName_content div div{position:relative;width:100%;height: 0.88rem;line-height: 0.88rem;border-bottom:0.01rem solid #efefef;}
.addName_content div div input{float: right;height: 0.36rem;padding: 0.25rem 0;text-align: right; font-size:0.28rem;}
.btn{width:90%;height:0.88rem;line-height: 0.88rem;font-size:0.32rem;background:#363636;border-radius: 0.1rem;color:#fff;text-align: center;margin:0 auto;}

/*********************************名单搜索****************************************************/
.nameHeader{position: fixed;top: 0;left: 0;width: 100%;height: 0.88rem;line-height: 0.88rem;font-size: 0.36rem;background: #da0e00;color: #fff;text-align: center;z-index: 999;}
.nameHeader>input{float: left;width: 80%;height: 0.3rem;line-height: 0.3rem;padding: 0.3rem 0.7rem;background: url(../img/search2.png) #fff no-repeat 0.2rem 0.16rem;background-size: 0.3rem;border: 0;border-radius: 0.08rem;margin-left: 5%;box-sizing: border-box;margin-top: 0.14rem;font-size: 0.26rem;}
.nameHeader .Cancel{position: relative;float: left;width: 15%;line-height: 0.6rem;text-align: center;font-size: 0.32rem;color: #fff;margin-top: 0.14rem;}
.SSNameList{margin-top: 0.88rem;display: none;}
.SSNameList .AllListTit{padding: 0 0.32rem;border-bottom: 0.01rem solid #efefef;box-sizing: border-box;}
.SSNameList .AllListTit li{float: left;font-size: 0.28rem;line-height: 0.71rem;color: #bd0f10;text-align: center;}
.SSNameList .AllListTit li:first-child,
.SSNameList .AllListCon li span:first-child{width: 20%;}
.SSNameList .AllListTit li:nth-child(2),
.SSNameList .AllListCon li span:nth-child(2){width: 33%;}
.SSNameList .AllListTit li:nth-child(3),
.SSNameList .AllListCon li span:nth-child(3){width: 30%;}
.SSNameList .AllListTit li:last-child,
.SSNameList .AllListCon li span:last-child{width: 17%;}
.SSNameList .AllListCon li{margin: 0 0.32rem;border-bottom: 0.01rem solid #efefef;box-sizing: border-box;text-align: center;overflow: hidden;}
.SSNameList .AllListCon li span{float: left;line-height: 0.88rem;font-size: 0.26rem;color: #333;}
.SSNameList .AllListCon li span:first-child {width: 20%;}





























