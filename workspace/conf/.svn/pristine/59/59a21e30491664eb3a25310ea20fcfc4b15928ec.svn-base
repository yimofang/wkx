body{background: #fafafa;}
html,body{-webkit-overflow-scrolling:touch;height:100%;overflow:auto;}
.Ove{overflow: hidden; text-overflow:ellipsis;white-space: nowrap;}
.none{display: none;}
.block{display: block;}
/***********************header***********************************/
.Header .HShare{float: right;width: 0.4rem;height: 0.4rem;margin-right:0.24rem;background: url(../img/share1.png) no-repeat;background-size: cover;background-position:right;margin-top: 0.24rem;}
/********************************container**************************************/
.Container{margin: 0.25rem 0.33rem 0.5rem;    overflow-y: scroll;-webkit-overflow-scrolling:touch;overflow-scrolling: touch;}
.Container .BannerImg img{display:block;width:100%;border-radius: 0.08rem;}
.meetPreview { -webkit-overflow-scrolling: touch; overflow-scrolling: touch; overflow-y: scroll; position: absolute; top: 0; bottom: 0; width: 100%; }
/*PreviewTitle 会议标题*/
.Container .PreviewTitle{background: #fff;padding: 0.24rem;box-shadow: 0px 3px 3.8px 0.2px rgba(231, 232, 234, 0.47);}
.Container .PreviewTitle h2{font-weight: bold;font-size: 0.32rem;}
.Container .PreviewTitle p{font-size: 0.24rem;color: #999cab;margin-top: 0.1rem;overflow: hidden;}
.Container .PreviewTitle p .Icon{float: left;width: 0.24rem; margin-top: 0.1rem;margin-right: 0.15rem;}
.Container .PreviewTitle p .Icon1{height:0.27rem;background: url(../img/Icon1.png) no-repeat;background-size: cover;}
.Container .PreviewTitle p .Icon2{height:0.24rem;background: url(../img/Icon2.png) no-repeat;background-size: cover;}
.Container .PreviewTitle p .Icon3{height:0.28rem;background: url(../img/Icon3.png) no-repeat;background-size: cover;}
.Container .PreviewTitle p span{float: left;width: 90%;}
.PreviewTitle .ditu{color:#333;}
/*PreviewContainer 会议内容*/
.Container .PreviewContainer{margin-top: 0.32rem;margin-bottom: 0.5rem;box-shadow: 0px 3px 3.8px 0.2px rgba(231, 232, 234, 0.47);}
/*PreviewContainer 会议内容--标题*/
.Container .PreviewContainer .PreConTitle{overflow: hidden;}
.Container .PreviewContainer .PreConTitle li{float: left;width: 25%;text-align: center;font-size: 0.28rem;color: rgb(134,137,151);background: transparent;}
.Container .PreviewContainer .PreConTitle li.active{color: #bd0f10;background: #fff;}
.Container .PreviewContainer .PreConTitle li span{display: block;width: 100%;height: 0.4rem;line-height: 0.4rem;margin: 0.2rem 0;box-sizing: border-box; }
.Container .PreviewContainer .PreConTitle li:nth-child(3) span{border-left:1px solid #e1e1e1;border-right:1px solid #e1e1e1;}
/*PreConListAll 会议内容切换*/
.Container .PreContainerALL{position: relative;}
/*PreConListAll 会议内容--描述*/
.PreDescribe .miaoshu{font-size: 0.24rem;margin-bottom: 0.2rem;line-height: 0.4rem;}
.PreDescribe .miaoshu p{font-size: 0.24rem !important;margin-bottom: 0.2rem!important;line-height: 0.4rem!important;}
.PreDescribe #Meetms img.miaoshuImg{display: block;width: 100%;margin-bottom: 0.16rem;}
.PreDescribe .GuestList p.GuestListTitle,.GuestListTitle{font-size:0.24rem;margin:0.26rem 0;color:#bd0f10;text-align: center;background: #fafafa;}
.PreDescribe .GuestList ul li{margin-bottom: 0.3rem;}
.PreDescribe .GuestList .GuestInfo{overflow: hidden;}
.PreDescribe .GuestList .GuestInfo img{float: left;width: 1.7rem;}
.PreDescribe .GuestList .GuestInfo h2{padding-left: 0.25rem;font-size: 0.28rem;font-weight: bold;line-height: 0.6rem;color: rgb(51,51,51);}
.PreDescribe .GuestList .GuestInfo p{ padding-left: 0.25rem;font-size: 0.22rem;color:rgb(78,78,78);line-height: 0.33rem;overflow: hidden;}
.PreDescribe .GuestList .InfoCon{text-indent:2em;font-size: 0.24rem; line-height: 0.35rem;color: rgb(153,156,171);margin-top: 0.15rem;text-align: justify; box-orient: vertical;}
/*PreConListAll 会议内容--流程*/
.PreConListAll {background: #fff;padding: 0.52rem 0.24rem;display: none;margin-bottom: 0.5rem;}
.PreDescribe img{float:left;}
.PreContainerALL>div.block{display: block;}
.PreProcess .PreConList{position: relative;    padding-bottom: 0.7rem;    padding-left: 0.28rem;}
.PreProcess .PreConList .Instructions{display: block;width:0.14rem;height: 0.14rem;background: url(../img/Instructions2.png) no-repeat;background-size: cover;position: absolute;top: 0.12rem;left: -0.07rem; }
.PreProcess .PreConList i.Active{background: url(../img/Instructions1.png) no-repeat;    background-size: cover;}
.PreProcess .PreConList:before{content: '';  position: absolute; top: 0; bottom: 0; width: 1px;  background: #ececec;top: 0; left: 0; }
.PreProcess .PreConList:first-child:before{top:0.12rem;}
.PreProcess .PreConList:last-child:before{height: 0.12rem;}
.PreProcess .PreConList .PreConListTime{overflow: hidden;}
.PreProcess .PreConList .PreConListTime span{font-size: 0.24rem;color: rgb(134,137,151);}
.PreProcess .PreConList .PreConListTime span.left{float: left;margin-left: -0.1rem;}
.PreProcess .PreConList .PreConListTime span.right{float: right;}
.PreProcess .PreConList .PreConListCon{text-align: justify;font-size: 0.26rem;color: rgb(51,51,51);width:100%;}
/*PreConListAll 会议内容--资料*/
.PreData ul li,.PreData .DataInfo{overflow: hidden;margin-bottom: 0.25rem;}
.PreData ul li img{float: left;width: 1.6rem;}
.PreData .DataInfo p{padding-left:0.25rem; margin-top: 0.15rem;line-height: 0.3rem;font-size: 0.24rem;color: rgb(153,156,171);}
.PreData .DataInfo .DataTitle{font-size: 0.26rem;line-height: 0.42rem;color: rgb(51,51,51); }
.PreData .DataInfo .DataNum span{float: left;margin-right: 0.1rem;}
.PreData .DataInfo .DataNum .DataLook{width: 0.29rem;height: 0.2rem;    margin-top: 0.05rem;background: url(../img/look.png) no-repeat;background-size: cover;}
.PreData .DataInfo .DataNum .DataShare{width: 0.24rem;height: 0.24rem;background: url(../img/share.png);margin-top: 0.04rem;margin-left: 0.2rem;background-size: cover;}
/*PreConListAll 会议内容--调查*/
.previewContent{height:auto;background:#fff;}
.previewContent i{color:#bd0f10;}
.previewContent .comName{ width: 92%;height: 0.3rem;line-height: 0.3rem;padding: 0.15rem 4%;background: #f4f4f4;border: none;font-size: 0.28rem;margin-top: 0.1rem;}
.previewContent ul li{width:90%;height:auto;padding:5% 0;border-bottom:0.01rem solid #efefef;}
.previewContent ul li p{font-size:0.26rem;line-height: 0.5rem;}
.previewContent ul li p:first-child{font-size:0.28rem;}
.previewContent input{width:0.32rem;height:0.32rem;line-height: 0.32rem;margin-right: 0.13rem;}
.previewContent .btn{display:none;width:100%;height:0.88rem;background:#1b1b1b;font-size:0.3rem;line-height: 0.88rem;color:#fff;text-align:center;border-radius: 0.1rem; margin-bottom: 0.5rem;}
/*nextmeet 下期预告*/
.nextmeetinfo{font-size: 0.24rem;margin-bottom: 0.2rem;line-height: 0.4rem;}
.nextmeetimg img{display: block;width: 100%;margin-bottom: 0.16rem;}
/*contact 联系报名*/
.PhoneAndName{overflow: hidden;position: absolute;width: 100%;bottom: 0;left: 0;}
.PhoneAndName p{float: left;width: 50%;height: 0.88rem;line-height: 0.88rem;font-size:0.3rem;color: #fff;text-align: center;}
.PhoneAndName .Phone a{background: #1b1b1b;float: left;width: 100%;height: 0.88rem;line-height: 0.88rem;font-size:0.3rem;color: #fff;text-align: center;}
.PhoneAndName .Name{background: #bd0f10}
.PhoneAndName .phone{background: #1b1b1b}

/*主办方弹框*/
.hostBg{width: 100%;top: 0;bottom: 0;left: 0;right: 0;position: fixed;z-index: 999;background: rgba(0,0,0,.2);}
.hostInfo{position: fixed;top:50%;left:50%;transform:translate(-50%,-50%);z-index: 1000;width: 5.6rem;height: 4.3rem;text-align: center;border-radius: 0.1rem;background: #fff;}
 .hostInfo p:nth-child(1){width:100%;height: 0.9rem;line-height: 0.9rem;text-align: center;background: #bd0f10;font-size: 0.32rem;color: #fff;border-top-left-radius: 0.1rem;border-top-right-radius: 0.1rem;}
 .hostInfo p:nth-child(2) a{font-size: 0.38rem;margin: 0.4rem 0;display: block;font-weight: 600;}
 .hostInfo p:nth-child(3){font-size: 0.28rem;font-weight: 600;}
 /*.hostInfo p:nth-child(4){width: 3.22rem;height: 0.68rem;line-height: 0.68rem;background: #1b1b1b;color: #fff;font-size: 0.3rem;margin-top: 0.4rem;margin-bottom: 0.16rem;padding-left: 0.1rem;border-radius: 1rem;text-align: center;margin: 0 auto;margin: 0.4rem auto 0;}*/
.hostSure {overflow: hidden;margin-top: 0.3rem;}
.hostSure span{float: left;width: 40%;height: 0.68rem;line-height: 0.68rem;font-size: 0.3rem;color: #fff;border-radius: 0.1rem;}
.hostSure span#close_phone{background:#1b1b1b;margin: 0 3% 0 7%;}
.hostSure span#sure_phoe{background:#bd0f10;margin: 0 7% 0 3%;}
.hostSure span#sure_phoe a{color: #fff;font-size: 0.3rem;}
/*报名*/
.addName_content{width: 100%;height:auto;background:#fff;font-size:0.28rem;}
.addName_content b{color:#bd0f10;}
.addName_content div{width:90%;height:auto;margin:0 auto;}
.addName_content div div{display:none;position:relative;width:100%;height: 0.88rem;line-height: 0.88rem;border-bottom:0.01rem solid #efefef;}
.addName_content div div input{float: right;height: 0.35rem;padding: 0.25rem 0;text-align: right;font-size: 0.28rem;}
.addName_content div div input.inputYes{padding-right:0.5rem;background:url(../img/yes.png) no-repeat;background-position:right;background-size: 0.32rem 0.22rem;} 
.nameBtn{position:fixed;bottom:0;width:100%;height:0.88rem;}
.nameBtn div{float: left;width: 50%;height: 0.88rem;line-height:0.88rem;font-size: 0.3rem;text-align: center;color: #fff;}
.nameBtn .back_btn{background: #1b1b1b}
.nameBtn .succ_btn{background: #bd0f10}

/*问卷调查弹框*/
.phoneBg{width: 100%;top: 0; bottom: 0;left: 0;right: 0;position: fixed;z-index: 999;background: rgba(0,0,0,.2);}
.phoneBg .phoneInfo{position: fixed;top: 50%;left: 50%;transform: translate(-50%,-50%);z-index: 1000;width: 5.6rem;height: 4.3rem;text-align: center;border-radius: 0.1rem;background: #fff;}
.phoneBg .phoneInfo .item{width: 100%;height: 0.9rem;line-height: 0.9rem;text-align: center;background: #bd0f10;font-size: 0.32rem;color: #fff;border-top-left-radius: 0.1rem;border-top-right-radius: 0.1rem;}
.phoneInfo .pNum,.phoneInfo .pInfo{width:80%;height:0.88rem;line-height: 0.88rem;margin:0.5rem auto;}
.phoneInfo .pNum span{float:left;}
.phoneInfo .pNum input{width: 65%;height: 0.68rem;border: 0.01rem solid #efefef;font-size: 0.28rem;padding-left: 0.1rem;border-radius: 0rem;}
.phoneInfo .pInfo span{width:40%;height:0.68rem;line-height: 0.68rem;display: inline-block;border-radius: 0.1rem;text-align: center;color:#fff;font-size:0.3rem;}
.phoneInfo .pInfo span.pClose{background:#1b1b1b;float:left;}
.phoneInfo .pInfo span.pSure{background:#bd0f10;float:right;}


.surTit{font-size: 0.36rem; text-align: center; margin-bottom: 0.2rem;}

/*地图*/
body,#container{width:100%;height:100%;margin:0;}
.position input{position: fixed;top:0.5rem; left:10%;font-size:0.28rem;width:80%;height:0.88rem;line-height: 0.88rem;padding-left:2%;}
.position p{position: fixed;right: 8%;top: 0.5rem;width: 0.88rem;height: 0.88rem;background: #ccc;font-size: 0.28rem;line-height: 0.88rem;text-align: center;}

iframe{width:100%;height:100%;}