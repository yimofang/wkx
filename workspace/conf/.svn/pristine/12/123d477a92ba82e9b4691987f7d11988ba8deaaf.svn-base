*{padding:0;margin:0;}
.myHead{position: relative;margin-top: 0.88rem;width:100%;height:4.8rem;text-align: center;background:url(../img/myHeadBg.png) no-repeat center center;background-size:8rem 4.8rem;}
.myHead .myPhoto img{position: relative;width:1.9rem;height:1.9rem;border-radius: 1.9rem;border:0.01rem solid #efefef;top: 0.93rem;margin: 0 auto;display: inline-block;}
.myHead .myName{position: relative;font-size: 0.32rem;color:#fff;top: 1.5rem;display: block;margin:0 auto;text-align: center;}
.myHead select{position: relative;top: 1.5rem;color:#333;background:#fff;height:0.4rem; outline:none;}
select option{border-bottom: 1px solid #eee; outline:none;}

.myInfo{width:100%;height:auto;margin-top:0.52rem;}
.myInfo ul li{width:90%;height:0.88rem;line-height: 0.88rem;padding:0 5%;font-size:0.28rem;background:#fff;border-bottom:0.01rem solid #efefef;}
.myInfo ul li a{display: block;width: 0%;height: 100%;   width:90%;height:0.88rem;line-height: 0.88rem;font-size:0.28rem;background:#fff;border-bottom:0.01rem solid #efefef}
.myInfo .meetRecord{padding-left: 0.5rem;background: url(../img/myIcon0.png) no-repeat;background-position: left;background-size: 0.3rem 0.34rem;}
.myInfo .infoCenter{padding-left: 0.5rem;background:url(../img/myIcon1.png) no-repeat;background-position:left;background-size: 0.3rem 0.34rem;}
.myInfo .modifyPW{padding-left: 0.5rem;background:url(../img/myIcon2.png) no-repeat;background-position:left;background-size: 0.3rem 0.34rem;}
.myInfo .exit{padding-left: 0.5rem;background:url(../img/myIcon3.png) no-repeat;background-position:left;background-size: 0.3rem 0.34rem;}
.myInfo .infoMore,.myInfo .moMore,.myInfo .reMore{display: inline-block;float:right;width:0.1rem;height:0.18rem;background:url(../img/more.png) no-repeat;background-size: cover;margin-top: 0.35rem;}



/*会议记录*/
.Content{top:0.88rem;}

/*修改密码*/
.modifyCon{margin-top: 1.12rem;}
.modifyCon ul li{width: 90%; height: 0.88rem;line-height: 0.88rem;background: #fff;padding: 0 5%;}
.modifyCon .modNew{margin-top: 0.16rem;border-bottom:0.01rem solid #efefef;}
.modifyCon .PW{font-size:0.3rem;width: 1.8rem;display: inline-block;}
.modifyCon .PWPut{width: 4.5rem;font-size:0.26rem;}
.del{width: 0.24rem;height: 0.24rem;float: right;margin-top: 0.33rem;background:url(../img/false.png) no-repeat;background-size: cover;}
.btn{width: 90%;height: 0.98rem;line-height: 0.98rem;background: #1b1b1b;color: #fff;text-align: center;font-size: 0.32rem;border-radius: 0.1rem;margin: 1.3rem auto 0;}

/*消息中心*/
.msgList{margin-top: 0.88rem;}
.msgList .infoList{position: relative;height: 1.4rem;width: 90%;padding: 0 5%;border-bottom: 1px solid #eaeaea;}
.msgList span{display: inline-block;}
.msgList .info{width: 90%;float:left;}
.msgList .info p{font-size: 0.26rem;white-space:nowrap; text-overflow:ellipsis; overflow:hidden;}
.msgList .delete{float:right;position: absolute;margin-top: 0.65rem;}
.msgList .delete img{width:0.3rem;height: 0.3rem;}
.info_title{line-height: 0.7rem}
.info_article{color:#999cab;line-height: 0.5rem}

.noMeet p,.unMsg p{font-size: 0.26rem;color: #999cab;margin-top: 0.4rem;}
.noMeet,.unMsg{position: absolute;top: 0;bottom: 0;left: 0;right: 0; margin: auto;text-align: center;width: 100%;height: 175px;}

/*消息详情*/
