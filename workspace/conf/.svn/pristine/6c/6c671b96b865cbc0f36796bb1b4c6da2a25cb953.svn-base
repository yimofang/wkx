}

/*addName*/
.addName_content{
	width: 100%;
	height:auto;
	background:#fff;
	margin-top: 0.88rem;
}
.addName_content b{
	color:#bd0f10;
}

.addName_content div{
	width:90%;
	height:auto;
	margin:0 auto;
}
.addName_content div div{
