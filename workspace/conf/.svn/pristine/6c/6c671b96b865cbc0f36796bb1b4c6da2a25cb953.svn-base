@charset "utf-8";
*{margin:0;padding:0;}
/*nav*/
.nav{
	width:100%;
	height:0.8rem;
	font-size:0.28rem;
	margin-top: 0.88rem;
	background: #fff;
}
.nav .nav_title{
	width:90%;
	height:0.8rem;
	line-height: 0.8rem;
	text-align: center;
	margin:0 auto;
}
.nav .nav_title li{
	width:33.3%;
	height:0.76rem;
	float:left;
}
.nav .nav_title li.current{
	border-bottom:0.04rem solid #bd0f10;
	color: #bd0f10;
}
/*bmqd*/
.bmqd{
	width:100%;
	height:auto;
}
.bmqd_content{
	position:relative;
	width:100%;
	height: 2rem;
	line-height: 2rem;
	text-align: center;
	margin-top: 0.16rem;
	background: #fff;
	border-bottom: 0.02rem solid #efefef;
}
.bmqd_content .bmqd_content_info{
	position: absolute;
	width:90%;
	height:1rem;
	margin:auto;
	margin-top: 0.5rem;
	margin-left:5%;
	font-size: 0.3rem;
}
.bmqd_content .bmqd_content_info div{
	width:33%;
	height: 1rem;
	line-height: 1rem;
	border-right:0.01rem solid #efefef;
	text-align: center;
	float: left;
}
.bmqd_content .bmqd_content_info div p{
	height:0.5rem;
	line-height: 0.5rem;
	
}
.bmqd_list{
	width: 100%;
	height:3.5rem;
	margin-top: 0.16rem;
	background: #fff;
	font-size: 0.28rem;
}
.bmqd_list div{
	width:90%;
	height:3.5rem;
	margin:0 auto;
}
.bmqd_list div ul li{
	width:100%;
	height:0.87rem;
	line-height: 0.87rem;
	border-bottom: 0.01rem solid #efefef;
}
.bmqd_list div ul li span:last-child{
	float:right;
}
/*submit*/
.btn{
	width:90%;
	height:0.98rem;
	line-height: 0.98rem;
	margin-left:5%;
	background:#1b1b1b;
	border-radius:0.1rem;
	font-size:0.32rem;
	color:#fff;
	text-align: center;
}
/*dytj*/
.dytj{
	width:100%;
	height:auto;
	background:#fff;
	margin-top: 0.16rem;
}
.dytj .dytj_title{
	width:90%;
	height:auto;
	padding:0.46rem 5%;
	border-bottom:0.01rem solid #efefef;
}

.dytj .dytj_title p:first-child{
	font-size:0.34rem;
	line-height: 0.49rem;
}
.dytj .dytj_title p:last-child{
	font-size:0.24rem;
	color:#c0c2cd;
	line-height: 0.39rem;
}
.dytj .dytj_content{
	width:100%;
	height:auto;
	font-size:0.28rem;	
}
.dytj .dytj_content ul li{
	width: 90%;
	height:0.88rem;
	line-height: 0.88rem;
	padding:0 5%;
	border-bottom:0.01rem solid #efefef;
}
.dytj_content ul li span:nth-child(2){
	float: right;
	padding-right:0.3rem;
	text-align: left;
	background: url(../img/more.png) no-repeat;
	background-position: right;
	background-size:0.1rem 0.18rem;
}

/*meetQueBlank页面*/
/*blank*/
.blankTitle{
	width:100%;
	height:1.3rem;
	margin-top: 0.88rem;
	background: #fff;
	
}
.blankTitle div{
	width:90%;
	height:1.3rem;
	line-height: 1.3rem;
	margin:0 auto;
	font-size: 0.28rem;
}
/*blankCompany*/
.blankCompany{
	width: 100%;
	height:auto;
	margin-top:0.16rem;
	background: #fff;
}
.blankCompany div{
	width:90%;
	height:0.8rem;
	line-height: 0.4rem;
	padding:0.2rem 0;
	margin:0 auto;
	border-bottom:0.01rem solid #efefef;
}
.blankCompany div p:nth-child(1){
	padding-left:0.5rem;
	background: url(../img/personImg.png) no-repeat;
	background-position: left;
	background-size: 0.4rem 0.4rem;
	font-size:0.22rem;
	color:#a1a0a0;
}
.blankCompany div p:nth-child(2){
	float:left;
	font-size:0.26rem;
}

/*meetQueSingle页面和meetQueDouble页面*/
.singleTitle,.doubleTitle{
	width:90%;
	height:1.3rem;
	line-height: 1.3rem;
	padding:0 5%;
	background:#fff;
	font-size:0.28rem;
	margin-top:0.88rem;
}
.singleAnswer,.doubleAnswer{
	width:100%;
	height: auto;
	background:#fff;
	font-size:0.28rem;
	margin-top: 0.16rem;
}
.singleAnswer ul li,.doubleAnswer ul li{
	width:90%;
	height:0.88rem;
	line-height: 0.88rem;
	margin:0 auto;
	border-bottom:0.01rem solid #efeffe;
}
.singleAnswer ul li span:nth-child(2),.doubleAnswer ul li span:nth-child(2){
	float:right;
	color:#868997;
}

/*meetDes*/
.descript{
	width: 100%;
    height: auto;
    margin-top: 0.88rem;
    font-size: 0.28rem;
}
.descript .desD{
	width: 90%;
    height: 0.65rem;
    line-height: 0.65rem;
    font-size: 0.24rem;
    margin: 0 5%;
}
.descript .desD_content{
	width: 90%;
    height: 2.1rem;
    padding: 0 5%;
    background: #fff;
}
.descript .desD_content>textarea{
	width: 100%;
    height: 1.6rem;
    padding: 0.25rem 0;
    font-size: 0.26rem;
    border: none;
}

/*meetFlow*/
.flowBtn{
	position: fixed;
	width:100%;
	height:0.88rem;
	bottom:0;
}
.flowBtn>span{
	width:50%;
	height:0.88rem;
	line-height: 0.88rem;
	text-align: center;
	color:#fff;
	font-size:0.28rem;
	display: inline-block;
	float:left;
}
.flowBtn>.addFlow{
	background: #bd0f10;
}
.flowBtn>.return{
	background: #1b1b1b;	
}
/*未添加*/
.flowList .unHave{
	width: 100%;
    margin-top: 3.54rem;
    text-align: center;
}
.flowList .unHave p{
	font-size:0.26rem;
	color:#999cab;
	margin-top: 0.4rem;
}
.flowInfo{
	width: 100%;
	height:auto;
	box-shadow: 0 0.01rem 0.05rem #ccc;
	font-size:0.28rem;
	margin-bottom: 1rem;
}
.flowInfo>p{
	width:100%;
	height:0.88rem;
	line-height: 0.88rem;
	text-align: center;
	font-size:0.28rem;
	color:#bd0f10;
	background: #fff;
}
.flowInfo>div{
	width:90%;
	height: auto;
	padding:0 5%;
	background: #fff;
	margin-bottom: 0.3rem;
	border-bottom:0.01rem solid #efefef;
	margin-top: 0.2rem;
}
.flowInfo ul li{
	border-bottom:0.01rem solid #efefef;
	height:0.88rem;
	line-height: 0.88rem;
}
.flowInfo ul li:last-child{
	height: auto;
}
.flowInfo .flow{
	color:#bd0f10;
	font-size:0.26rem;
	height: 0.68rem;
	line-height: 0.68rem;
}
.flowInfo .flow>span:nth-child(1){
	padding-left:0.46rem;
	background:url(../img/flow.png) no-repeat;
	background-position: left;
	background-size: 0.29rem 0.3rem;
}
.flowInfo .flow>span:nth-child(2){
	position: relative;
    right: -4.5rem;
}
.flowInfo .flow span:nth-child(3){
	float:right;
}
.fStartTime span:nth-child(2),.fEndTime span:nth-child(2){
	float:right;
	color:#868997;
	font-size:0.28rem;
}
.flowInfo b{
	color:#bd0f10;
}
.flowInfo .flowDes p{
	width: 100%;
    height: auto;
    border: none;
    font-size: 0.28rem;
    line-height: 0.4rem;
    overflow: auto;
}
.flowInfo div ul li input{
	float:right;
	height: 0.86rem;
	text-align: right;
	font-size:0.28rem;
}
.addFlowInfo{
	width: 100%;
    height: auto;
    font-size: 0.28rem;
    margin-top: 0.88rem;
}
.addFlowInfo .flowDes{
	height: 2.67rem;
}
.addFlowInfo .flowDes textarea{
	width: 95%;
    height: 1.7rem;
    border: none;
    background: #f4f4f4;
    padding: 0.12rem 2.5%;
    font-size: 0.28rem;
}
.addFlowInfo ul li:last-child{
    padding-bottom: 0.3rem;
}
.save,.editFlow .editSave{
	position: fixed;
	width:50%;
	height:0.88rem;
	background: #bd0f10;
	line-height: 0.88rem;
	text-align: center;
	font-size: 0.3rem;
	bottom:0;
	right:0;
	color:#fff;
}
.cancel,.editFlow .editCancel{
	position: fixed;
	width:50%;
	height:0.88rem;
	background: #1b1b1b;
	line-height: 0.88rem;
	text-align: center;
	font-size: 0.3rem;
	bottom:0;
	left:0;
	color:#fff;
}
.havaMeetFlow{
	margin-top:0.88rem;
}
.havaMeetFlow .flowInfo .flow{
	position: relative;
}
.havaMeetFlow .flowInfo .fInfoList .flow span:nth-child(2){
	position: absolute;
    right: 0.62rem;
    display: inline-block;
    width: 0.3rem;
    height: 0.3rem;
    background: url(../img/edit.png) no-repeat;
    background-size: cover;
    background-position: center;
    margin-top: 0.19rem;
}
.havaMeetFlow .flowInfo .fInfoList .flow span:nth-child(3){
	float:right;
	width:0.3rem;
	height:0.3rem;
	display: inline-block;
	background: url(../img/delete.png) no-repeat;
	background-size: cover;
	margin-top: 0.19rem;
}
.havaMeetFlow .flowInfo .fInfoList .flowDes .flowDesP{
	max-height: 1.89rem;
    line-height: 0.44rem;
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 4;
    overflow: hidden;
    width:100%;
    text-align: left;
}
/*删除遮罩*/
.havaMeetFlow .delShade,.guestList .gueDelShade{
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    position: fixed;
    z-index: 999;
    background: rgba(0,0,0,.2);
}
.havaMeetFlow .delShade .delShadeCon,
.guestList .gueDelShade .gueDelCon
{
	position: absolute;
	top: 50%;
	left: 50%;
    z-index: 1000;
    width: 5.57rem;
    height: 3.7rem;
    transform: translate(-50%,-50%);
    border-radius: 0.1rem;
    background: #fff;
}
.delShadeCon .top,.gueDelCon .top{
	width: 5.57rem;
    height: 0.9rem;
    line-height: 0.9rem;
    text-align: center;
    background: #bd0f10;
    font-size: 0.32rem;
    color: #fff;
    border-top-left-radius: 0.1rem;
    border-top-right-radius: 0.1rem;
}
.delShadeCon .content p:nth-child(1),
.gueDelCon .content p:nth-child(1){
	font-size:0.28rem;
	margin-top:0.5rem;
	text-align: center;
}
.delShadeCon .content p:nth-child(2),
.gueDelCon .content p:nth-child(2){
	margin: 0.57rem auto 0;
	width:4.7rem;
	font-size: 0.3rem;
}
.delShadeCon .content p:nth-child(2)>.conCel,
.gueDelCon .content p:nth-child(2)>.gueCel{
	width:2.3rem;
	height:0.68rem;
	line-height: 0.68rem;
	text-align: center;
	float: left;
	display: inline-block;
	background: #1b1b1b;
	color:#fff;
	border-radius: 0.1rem;
}
.delShadeCon .content p:nth-child(2)>.conSure{
	width:2.3rem;
	height:0.68rem;
	line-height: 0.68rem;
	text-align: center;
	float: right;
	display: inline-block;
	background: #1b1b1b;
	color:#fff;
	border-radius: 0.1rem;
}
.gueDelCon .content p:nth-child(2)>.gueSure{
	width:2.3rem;
	height:0.68rem;
	line-height: 0.68rem;
	text-align: center;
	float: right;
	display: inline-block;
	background: #bd0f10;
	color:#fff;
	border-radius: 0.1rem;
}
/*编辑会议流程*/
.editFlow{
	width: 100%;
    height: auto;
    font-size: 0.28rem;
    margin-top: 0.88rem;
}
.editFlow li.flowDes{
	padding-bottom:0.3rem;
}
.editFlow li.flowDes .flowDesTxt{
	width: 95%;
    height: 1.7rem;
    border: none;
    background: #f4f4f4;
    padding: 0.12rem 2.5%;
    font-size: 0.28rem;
}

/*meetGuest*/
.guestBtn{
	position: fixed;
	width:100%;
	bottom:0;
	font-size: 0.3rem;
	color: #fff;
}
.guestBtn .addGuest{
    width: 50%;
    height: 0.88rem;
    line-height: 0.88rem;
    text-align: center;
    background: #bd0f10;
	display:inline-block;
	float:left;
}
.guestBtn .returnGuest{
    width: 50%;
    height: 0.88rem;
    line-height: 0.88rem;
    text-align: center;
    background: #1b1b1b;
	display:inline-block;    
	float:left;
}
.guestList .unHaveGuest{
	width: 100%;
    margin-top: 3.54rem;
    text-align: center;
}
.guestList .unHaveGuest p{
	font-size: 0.26rem;
    color: #999cab;
    margin-top: 0.4rem;
}
.guestInfo{
	width: 100%;
    height: auto;
    font-size: 0.28rem;
    margin-top: 0.88rem;
}

.guestInfo div{
	width: 90%;
    height: auto;
    padding: 0 5% 5%;
    background: #fff;
    margin-bottom: 0.3rem;
    box-shadow: 0 0.01rem 0.05rem #ccc;
}
.guestInfo div ul li{
	position: relative;
	width:100%;
	height: 0.88rem;
	line-height: 0.88rem;
	border-bottom: 0.01rem solid #efefef;
}

#EperImgHead{width: 0.6rem;
	height: 0.6rem;
	position: absolute;
