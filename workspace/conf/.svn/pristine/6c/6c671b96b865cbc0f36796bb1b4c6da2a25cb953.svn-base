    background-size: cover;
    margin-top: 0.29rem;
}
.guestInfo div ul li input{
	float:right;
	height: 0.86rem;
	text-align: right;
	font-size:0.28rem;
}
.guestInfo .introduce textarea{
	width:95%;
	height:1.7rem;
	border:none;
	background: #f4f4f4;
	padding:0.12rem 2.5%;
	font-size:0.28rem;
}
.guestInfo b{
	color:#bd0f10;
}
.guestInfo .guestPhoto{
	width:100%;
	position: absolute;
	float:right;
    opacity: 0;
    display: inline-block;
    right:0;
    z-index: 1;
}
.haveGuest{
	width: 100%;
    height: auto;
    box-shadow: 0 0.01rem 0.05rem #ccc;
    font-size: 0.28rem;
    margin-top: 0.88rem;
}
.haveGuest .guestName .guestNameInfo,
.haveGuest .guestHonor .guestHonorInfo{
	float:right;
}
.haveGuest .introduce{
	border-bottom: none;
	height: auto;
}
.haveGuest .introduce p{
	max-height: 1.89rem;
    line-height: 0.44rem;
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 4;
    overflow: hidden;
    width: 100%;
