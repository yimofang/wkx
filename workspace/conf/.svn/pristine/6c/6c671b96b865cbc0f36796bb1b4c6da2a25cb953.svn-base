.enterName{
	width:90%;
	height:auto;
	padding:0 5% 5%;
	background:#fff;
	margin-top:0.88rem;
}
.enterName b{
	color:#bd0f10;
}
.enterName ul{
	width:100%;
}
.nStartTime,.nEndTime{
	width:100%;
