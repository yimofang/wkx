}

/*userDefined页面*/
.userDefined{
	width:90%;
	height:1rem;
	padding:0 5%;
	margin-top: 1.12rem;
	background: #fff;
	font-size:0.26rem;
}
.userDefined input{
	width:100%;
	height: 1rem;
	line-height:1rem;
	border:none; 
}
/*meetDes addImg*/
/*上传图片通用样式*/
.meetDesImg{width:90%;margin:0.2rem auto 0.8rem;background: #fff;padding: 0.2rem 5%;overflow: auto;}
.meetDesImg .uploadForm{width:100%;height:auto;}
.meetDesImg .upload-ul{ position: relative;  display: inline-block;zoom:1; width:100%;height:auto;}
.meetDesImg .upload-ul .upload-pick{ position: fixed;width:2rem;height:2rem;top:4rem; border: 0.02rem dashed #efefef;-moz-border-radius:0.1rem; -webkit-border-radius:0.1rem; border-radius:0.1rem; overflow: hidden; z-index:999;}
.meetDesImg .upload-ul li.diyUploadHover{width:3rem;height:3rem;text-align: center;margin:0.15rem;float:left;display: block;z-index: 1;}
.meetDesImg .upload-ul li.diyUploadHover:nth-child(2),.meetDesImg .upload-ul li.diyUploadHover:nth-child(3){margin-top:2rem;}
.meetDesImg .upload-pick{position: fixed; background: url(../img/z_add.png) no-repeat  center center;}
.webuploader-pick{position: relative;display: inline-block;vertical-align: top; width: 100%; height: 100%;}
.webuploader-container{ position: relative; width: 100%; height: 100%; }
.webuploader-container label{position: absolute;left: 0;top: 0;width: 100%;}
.webuploader-element-invisible{opacity: 0;width: 100%;height: 100%;clip: rect(1px 1px 1px 1px);clip: rect(1px,1px,1px,1px);}
.viewThumb{ position:relative;width: 100%;height: 100%;overflow:hidden;}
.viewThumb img{ width: 100%;height: 100%}
.diyBar{ position: absolute; display:none; top: 0;left: 0;width: 100%;height: 100%;background: url(../img/bgblack.png);z-index: 3;}
.diyProgress{ position: absolute; left: 0;top: 33px;width: 100%;height: 24px;line-height:24px;font-size: 14px;text-align: center;color: #FFF;background:rgba(10,168,241,.7); z-index: 3;}
.diyControl{ position: absolute; display:none; left: 0;bottom: 0;width: 100%;height: 24px;line-height:24px;font-size: 14px;background: url(../img/bgblack.png); z-index: 3; }
.viewThumb:hover .diyControl{ display: block; }
.diyControl span{ display: inline-block; padding: 6.5px 13px; width: 12px; height: 11px; }
.diyControl span i{ display: block; width: 12px; height: 11px; opacity: .7; }
.diyControl span i:hover{ opacity: 1; cursor: pointer; }
.diyLeft{ margin-left: 3px; }
.diyLeft{ margin-right: 3px; }
.diyLeft i{ margin-left: 3px; background: url(../img/upload-icon1.png) no-repeat 0 0; }
.diyCancel i{ background: url(../img/upload-icon1.png) no-repeat 0 -11px; }
.diyRight i{ margin-right: 3px; background: url(../img/upload-icon1.png) no-repeat 0 -22px; }
.goods-edit .upload-tip{ font: 12px/23px "Microsoft YaHei"; color: #a3a3a3;vertical-align: top; }

