    height: auto;
    float: right;
    color: #83838b;
    z-index: 1000;
    margin-top: 0.11rem;
    border: none;
    padding:0;
}
.G_img img{
	width: 0.6rem;
	height: 0.6rem;
	position: absolute;
	right: 0;
	border-radius: 0.3rem;
}
.guePhoto .perImg .guePhotoUp{
	float: right;
