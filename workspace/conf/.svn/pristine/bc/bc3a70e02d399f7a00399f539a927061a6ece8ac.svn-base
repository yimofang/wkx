/*userDefined页面*/
.userDefined{
	width:90%;
	height:1rem;
	padding:0 5%;
	margin-top: 1.12rem;
	background: #fff;
	font-size:0.26rem;
}
.userDefined input{
	width:100%;
	height: 1rem;
	line-height:1rem;
	border:none; 
}

