@charset "utf-8"
*{margin:0;padding:0;}
/*nav*/
.nav{
	width:100%;
	height:0.8rem;
	font-size:0.28rem;
	margin-top: 0.88rem;
	background: #fff;
}
.nav .nav_title{
	width:90%;
	height:0.8rem;
	line-height: 0.8rem;
	text-align: center;
	margin:0 auto;
}
.nav .nav_title li{
	width:33.3%;
	height:0.76rem;
	float:left;
}
.nav .nav_title li.current{
	border-bottom:0.04rem solid #bd0f10;
	color: #bd0f10;
}
/*bmqd*/
.bmqd{
	width:100%;
	height:auto;
}
.bmqd_content{
	position:relative;
	width:100%;
	height: 2rem;
	line-height: 2rem;
	text-align: center;
	margin-top: 0.16rem;
	background: #fff;
	border-bottom: 0.02rem solid #efefef;
}
.bmqd_content .bmqd_content_info{
	position: absolute;
	width:90%;
	height:1rem;
	margin:auto;
	margin-top: 0.5rem;
	margin-left:5%;
	font-size: 0.3rem;
}
.bmqd_content .bmqd_content_info div{
	width:33%;
	height: 1rem;
	line-height: 1rem;
	border-right:0.01rem solid #efefef;
	text-align: center;
	float: left;
}
.bmqd_content .bmqd_content_info div p{
	height:0.5rem;
	line-height: 0.5rem;
	
}
.bmqd_list{
	width: 100%;
	height:3.5rem;
	margin-top: 0.16rem;
	background: #fff;
	font-size: 0.28rem;
}
.bmqd_list div{
	width:90%;
	height:3.5rem;
	margin:0 auto;
}
.bmqd_list div ul li{
	width:100%;
	height:0.87rem;
	line-height: 0.87rem;
	border-bottom: 0.01rem solid #efefef;
}
.bmqd_list div ul li span:last-child{
	float:right;
}
/*submit*/
.btn{
	width:90%;
	height:0.98rem;
	line-height: 0.98rem;
	margin-left:5%;
	background:#1b1b1b;
	border-radius:0.1rem;
	font-size:0.32rem;
	color:#fff;
	text-align: center;
}
/*dytj*/
.dytj{
	width:100%;
	height:auto;
	background:#fff;
	margin-top: 0.16rem;
}
.dytj .dytj_title{
	width:90%;
	height:auto;
	padding:0.46rem 5%;
	border-bottom:0.01rem solid #efefef;
}

.dytj .dytj_title p:first-child{
	font-size:0.34rem;
	line-height: 0.49rem;
}
.dytj .dytj_title p:last-child{
	font-size:0.24rem;
	color:#c0c2cd;
	line-height: 0.39rem;
}
.dytj .dytj_content{
	width:100%;
	height:auto;
	font-size:0.28rem;	
}
.dytj .dytj_content ul li{
	width: 90%;
	height:0.88rem;
	line-height: 0.88rem;
	padding:0 5%;
	border-bottom:0.01rem solid #efefef;
}
.dytj_content ul li span:nth-child(2){
	float: right;
	padding-right:0.3rem;
	text-align: left;
	background: url(../img/more.png) no-repeat;
	background-position: right;
	background-size:0.1rem 0.18rem;
}

/*meetQueBlank页面*/
/*blank*/
.blankTitle{
	width:100%;
	height:1.3rem;
	margin-top: 0.88rem;
	background: #fff;
	
}
.blankTitle div{
	width:90%;
	height:1.3rem;
	line-height: 1.3rem;
	margin:0 auto;
	font-size: 0.28rem;
}
/*blankCompany*/
.blankCompany{
	width: 100%;
	height:auto;
	margin-top:0.16rem;
	background: #fff;
}
.blankCompany div{
	width:90%;
	height:0.8rem;
	line-height: 0.4rem;
	padding:0.2rem 0;
	margin:0 auto;
	border-bottom:0.01rem solid #efefef;
}
.blankCompany div p:nth-child(1){
	padding-left:0.5rem;
	background: url(../img/personImg.png) no-repeat;
	background-position: left;
	background-size: 0.4rem 0.4rem;
	font-size:0.22rem;
	color:#a1a0a0;
}
.blankCompany div p:nth-child(2){
	float:left;
	font-size:0.26rem;
}

/*meetQueSingle页面和meetQueDouble页面*/
.singleTitle,.doubleTitle{
	width:90%;
	height:1.3rem;
	line-height: 1.3rem;
	padding:0 5%;
	background:#fff;
	font-size:0.28rem;
	margin-top:0.88rem;
}
.singleAnswer,.doubleAnswer{
	width:100%;
	height: auto;
	background:#fff;
	font-size:0.28rem;
	margin-top: 0.16rem;
}
.singleAnswer ul li,.doubleAnswer ul li{
	width:90%;
	height:0.88rem;
	line-height: 0.88rem;
	margin:0 auto;
	border-bottom:0.01rem solid #efeffe;
}
.singleAnswer ul li span:nth-child(2),.doubleAnswer ul li span:nth-child(2){
	float:right;
	color:#868997;
}

/*meetDes*/
.descript{
	width: 100%;
    height: auto;
    margin-top: 0.88rem;
    font-size: 0.28rem;
}
.descript .desD{
	width: 90%;
    height: 0.65rem;
    line-height: 0.65rem;
    font-size: 0.24rem;
    margin: 0 5%;
}
.descript .desD_content{
	width: 90%;
    height: 2.1rem;
    padding: 0 5%;
    background: #fff;
}
.descript .desD_content>textarea{
	width: 100%;
    height: 1.6rem;
    padding: 0.25rem 0;
    font-size: 0.26rem;
    border: none;
}
/*meetDes addImg*/
.container {
    width: 100%;
    height: 100%;
    overflow: auto;
    clear: both;
}
.container>p{
  	width: 90%;
    height: 0.65rem;
    line-height: 0.65rem;
    font-size: 0.24rem;
    margin: 0 5%;
}
.z_photo {
    width: 90%;
    height: 1rem;
    padding: 0.3rem 5%;
    overflow: auto;
    clear: both;
    margin: 0 auto;
    background:#fff;
}

