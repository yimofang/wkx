}*/

.z_file {
    display: block;
    width: 1rem;
    height: 1rem;
    background: url(../img/z_add.png) no-repeat;
    background-size: 100% 100%;
    float: left;
}
.z_file input::-webkit-file-upload-button {
    width: 1rem;
    height: 1rem;
    border: none;
    position: absolute;
    outline: 0;
    opacity: 0;
}
.z_file input#file {
    display: block;
    width: auto;
    border: 0;
    vertical-align: middle;
}
.z_pList{
	width: 90%;
    padding: 0 5%;
    height: 5rem;
    overflow: auto;
    background: #fff;
}
.z_pList .z_addImg
{
	float: left;
    width: 45%;
    height: 50%;
    overflow: hidden;
    margin-right: 5%;
    margin-bottom: 5%;
}
.z_pList img{
	width:100%;
	height:100%;
}
/*遮罩层*/
.z_mask {
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, .5);
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999;
    display: none;
}

.z_alert {
    width: 3rem;
    height: 2rem;
    border-radius: .2rem;
    background: #fff;
    font-size: .24rem;
    text-align: center;
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -1.5rem;
    margin-top: -2rem;
}

.z_alert p:nth-child(1) {
    line-height: 1.5rem;
}

.z_alert p:nth-child(2) span {
    display: inline-block;
    width: 49%;
    height: .5rem;
    line-height: .5rem;
    float: left;
    border-top: 1px solid #ddd;
}

.z_cancel {
    border-right: 1px solid #ddd;
}

/*meetFlow*/
.addFlow{
	width:100%;
	height:0.88rem;
	line-height: 0.88rem;
	text-align: center;
	color:#bd0f10;
	font-size:0.28rem;
	background: #fff;
	margin-top: 0.88rem;
}
.return{
	width:100%;
	height:0.88rem;
	line-height: 0.88rem;
	text-align: center;
	font-size:0.28rem;
	background: #fff;
	border-top:0.01rem solid #efefef;
}
.flowInfo{
	width: 100%;
	height:auto;
	box-shadow: 0 0.01rem 0.05rem #ccc;
	font-size:0.28rem;
	margin-top:0.88rem;
}
.flowInfo>p{
	width:100%;
	height:0.88rem;
	line-height: 0.88rem;
	text-align: center;
	font-size:0.28rem;
	color:#bd0f10;
	background: #fff;
}
.flowInfo>div{
	width:90%;
	height: auto;
	padding:0 5%;
	background: #fff;
	margin-bottom: 0.3rem;
	border-bottom:0.01rem solid #efefef;
	margin-top: 0.2rem;
}
.flowInfo ul li{
	border-bottom:0.01rem solid #efefef;
	height:0.88rem;
	line-height: 0.88rem;
}
.flowInfo ul li:last-child{
	height: auto;
}
.flowInfo .flow{
	color:#bd0f10;
	font-size:0.26rem;
}
.flowInfo .flow>span:nth-child(1){
	padding-left:0.46rem;
	background:url(../img/flow.png) no-repeat;
	background-position: left;
	background-size: 0.29rem 0.3rem;
}
.flowInfo .flow>span:nth-child(2){
	position: relative;
    right: -4.5rem;
}
.flowInfo .flow span:nth-child(3){
	float:right;
}
.fStartTime span:nth-child(2),.fEndTime span:nth-child(2){
	float:right;
	color:#868997;
	font-size:0.28rem;
}
.flowInfo b{
	color:#bd0f10;
}
