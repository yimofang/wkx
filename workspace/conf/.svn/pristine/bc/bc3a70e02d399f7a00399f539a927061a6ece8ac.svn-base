}*/
.flowInfo .flowDes p{
	width: 100%;
    height: auto;
    border: none;
    font-size: 0.28rem;
    line-height: 0.4rem;
    overflow: auto;
    padding-bottom: 0.5rem;
}
.flowInfo div ul li input{
	float:right;
	height: 0.86rem;
	text-align: right;
	font-size:0.28rem;
}
/*添加流程*/
.addFlowInfo{
	width: 100%;
    height: auto;
    font-size: 0.28rem;
    margin-top: 0.88rem;
}
.addFlowInfo .flowDes textarea{
	width: 95%;
    height: 1.7rem;
    border: none;
    background: #f4f4f4;
    padding: 0.12rem 2.5%;
    font-size: 0.28rem;
}
.addFlowInfo ul li:last-child{
    padding-bottom: 0.3rem;
}
.save{
	width:90%;
	height:0.98rem;
	line-height: 0.98rem;
	text-align: center;
	font-size:0.32rem;
	background: #1b1b1b;
	color:#fff;
	margin:2rem auto 0.16rem;
	border-radius: 0.1rem;
}
.cancel{
	width:90%;
	height:0.98rem;
	line-height: 0.98rem;
	text-align: center;
	font-size:0.32rem;
	background: #bd0f10;
	color:#fff;
	margin:0 auto;
	border-radius: 0.1rem;
}
/*meetGuest*/
.addGuest{
    width: 100%;
    line-height: 0.88rem;
    text-align: center;
    color: #bd0f10;
    font-size: 0.28rem;
    background: #fff;
    margin-top: 0.88rem;
    border-bottom:0.01rem solid #efefef;
}
.gueReturn{
    width: 100%;
    line-height: 0.88rem;
    text-align: center;
    font-size: 0.28rem;
    background: #fff;
}
.guestInfo{
	width: 100%;
    height: auto;
    box-shadow: 0 0.01rem 0.05rem #ccc;
    font-size: 0.28rem;
    margin-top: 0.88rem;
}

.guestInfo div{
	width: 90%;
    height: 6.7rem;
    padding: 0 5%;
    background: #fff;
    margin-bottom: 0.3rem;
    border-bottom: 0.01rem solid #efefef;
}
.guestInfo div ul li{
	position: relative;
	width:100%;
	height: 0.88rem;
	line-height: 0.88rem;
	border-bottom: 0.01rem solid #efefef;
}
#perImg{
	width: 0.55rem;
    height: 0.6rem;
    line-height: 0.6rem;
    float: right;
    color: #83838b;
    z-index: 1000;
    margin-top: 0.11rem;
    border: none;
}
#perImg p{
	float: right;
    position: absolute;
    right: 0;
}
#perImg #perImgHead{
	width:0.5rem;
	height:0.5rem;
	position: absolute;
	right:0;
	margin-top:0.05rem;
}
.guestInfo ul li:first-child
{
	position: relative;
	color:#bd0f10;
	font-size:0.3rem

}

.guestInfo ul li:first-child span:nth-child(3){
	position: absolute;
    color: #bd0f10;
    font-size: 0.28rem;
    float: right;
    right: 1rem;
    display: inline-block;
	
}
.guestInfo ul li:first-child span:nth-child(4){
	position: absolute;
    color: #bd0f10;
    font-size: 0.28rem;
    float: right;
    right: 0;
}
.guestInfo div ul li img{
	width:0.3rem;
	height: 0.3rem;

}
.guestInfo div ul li input{
	float:right;
	height: 0.86rem;
	text-align: right;
	font-size:0.28rem;
}
.guestInfo .introduce textarea{
	width:95%;
	height:1.7rem;
	border:none;
	background: #f4f4f4;
	padding:0.12rem 2.5%;
	font-size:0.28rem;
}
.guestInfo b{
	color:#bd0f10;
}
.guestInfo .guestPhoto{
	width:100%;
	position: absolute;
	float:right;
    opacity: 0;
    display: inline-block;
    right:0;
    z-index: 1;
}
.haveGuest{
	width: 100%;
    height: auto;
    box-shadow: 0 0.01rem 0.05rem #ccc;
    font-size: 0.28rem;
    margin-top: 0.88rem;
}
.haveGuest>p{
	width:100%;
	height:0.88rem;
	line-height: 0.88rem;
	text-align: center;
	font-size:0.28rem;
	color:#bd0f10;
	background: #fff;
}
.haveGuestInfo{
	margin-top: 0.2rem;
}
.haveGuestInfo>div{
	width: 90%;
    height: auto;
    padding: 0 5%;
    background: #fff;
    margin-bottom: 0.3rem;
    border-bottom: 0.01rem solid #efefef;
}
.haveGuestInfo ul li:last-child{
	border-bottom:none;
	height:auto;
	padding-bottom: 0.3rem;
}
.haveGuestInfo ul li:last-child p{
	max-height: 1.89rem;
    line-height: 0.44rem;
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 4;
    overflow: hidden;
}
/*addName*/
.addName_content{
	width: 100%;
	height:auto;
	background:#fff;
	margin-top: 0.88rem;
}
.addName_content b{
	color:#bd0f10;
}

.addName_content div{
	width:90%;
	height:auto;
	margin:0 auto;
}
.addName_content div div{
	position:relative;
	width:100%;
	height: 0.88rem;
	line-height: 0.88rem;
	border-bottom:0.01rem solid #efefef;
}
.addName_content div div input{
	float: right;
	height:0.86rem;
	text-align:right;
}

/*隐藏内容*/
.ifSignUp,.ifCheckIn{
	position:absolute;
    top:0;
    bottom:0;
    left:0;
    right:0;
    width:100%;
    background:rgba(0, 0, 0, 0.2);
    z-index:999;
}
.shade{
	position:absolute;
    top:0;
    bottom:1.75rem;
    left:0;
    right:0;
    width:100%;
}
.ifEnter{
	position: fixed;
	width:100%;
	height:1.75rem;
	bottom:0rem;
	background: #fff;
	font-size: 0.28rem;
	z-index:1000;
}
.ifEnter div{
	width:100%;
	height:0.88rem;
	line-height: 0.88rem;
	text-align: center;
	color:#5d5c5c;
	border-bottom:0.01rem solid #efefef;
}

/*enternName页面*/
/*enterName*/
.enterName{
	width:90%;
	height:auto;
	padding:0 5% 5%;
	background:#fff;
	margin-top:0.88rem;
}
.enterName b{
	color:#bd0f10;
}
.enterName ul{
	width:100%;
}
.nStartTime,.nEndTime{
	width:100%;
	height:0.88rem;
	line-height: 0.88rem;
	font-size:0.28rem;
	border-bottom:0.01rem solid #efefef;
}
.nStartTime span:nth-child(2),.nEndTime span:nth-child(2){
	float:right;
	color:#c0c2cd;
}
.enterInfo{
	width:100%;
	height:auto;
	font-size:0.28rem;
}
.enterInfo>span{
	height:0.88rem;
	line-height: 0.88rem;
}
.enterInfo .infoContent{
	width:100%;
	height:1.76rem;
	padding-top:0.1rem;
}
.enterInfo .infoContent ul li{
	position: relative;
	width:1.36rem;
	height:0.64rem;
	line-height: 0.64rem;
	text-align: center;
	color:#bd0f10;
	font-size:0.24rem;
	float: left;
	border-radius: 0.05rem;
	margin-right:0.28rem;
	margin-bottom: 0.32rem;
	border:0.02rem solid #bd0f10;
}
.enterInfo .infoContent button{
	position: absolute;
	background: url(../img/minus.png) no-repeat;
	background-size: cover;
	right: -0.12rem;
    top: -0.12rem;
	width: 0.24rem;
	height:0.24rem;
	border:none;
}
.defind{
	width:100%;
	height:0.88rem;
	margin-top: 0.5rem;
}
.defind ul li{
	position: relative;
	width: 1.36rem;
    height: 0.64rem;
    line-height: 0.64rem;
    text-align: center;
    color: #c0c2cd;
    font-size: 0.24rem;
    float:left;
    border-radius: 0.05rem;
    margin-right: 0.28rem;
    margin-bottom: 0.32rem;
    border: 0.02rem solid #c0c2cd;
}
.defind button{
	position: absolute;
	background: url(../img/add.png) no-repeat;
	background-size: cover;
	right: -0.12rem;
    top: -0.12rem;
	width: 0.24rem;
	height:0.24rem;
	border:none;
}
